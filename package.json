{
  "name": "tarkov.dev",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prestart": "npm run prebuild",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "eject": "react-scripts eject",
    "prebuild": "node scripts/update-props.js && node --experimental-loader ./scripts/custom-loader.mjs scripts/cache-api-data.mjs && node scripts/get-version.js && node scripts/get-contributors.js && node scripts/build-sitemap.js && node scripts/generate-thumbnails.js",
    "postbuild": "node scripts/build-redirects.js && node scripts/critical.js",
    "stage": "npx rimraf build && npm run build && npm run preview",
    "preview": "npx serve build -l 3001 -s",
    "critical": "node scripts/critical.js",
    "cache-api-data": "node --experimental-loader ./scripts/custom-loader.mjs scripts/cache-api-data.mjs"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@mdi/js": "^6.9.96",
    "@mdi/react": "^1.6.1",
    "@mui/lab": "^5.0.0-alpha.140",
    "@mui/material": "^5.14.4",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@tippyjs/react": "^4.2.6",
    "classnames": "^2.3.2",
    "critical": "^4.0.1",
    "cross-fetch": "^4.0.0",
    "dayjs": "^1.11.9",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^6.5.1",
    "fuse.js": "^6.6.2",
    "i18next": "^23.4.2",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "intersection-observer": "^0.12.2",
    "leaflet": "^1.9.4",
    "leaflet.awesome-markers": "2.0.5",
    "leaflet-fullscreen": "^1.0.2",
    "lodash.debounce": "^4.0.8",
    "rc-slider": "^10.1.1",
    "react": "^18.2.0",
    "react-cookie-consent": "^8.0.1",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-i18next": "^13.1.2",
    "react-intersection-observer": "^9.5.2",
    "react-loader-spinner": "^5.3.4",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.15.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.4",
    "react-simple-image-viewer": "1.2.2",
    "react-switch": "^7.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-table": "^7.8.0",
    "react-zoom-pan-pinch": "^3.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "sharp": "^0.32.4",
    "source-map-explorer": "^2.5.3",
    "victory": "^36.6.10"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@github/|@primer/|tippy\\.js|rc-slider).*"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4
  }
}
