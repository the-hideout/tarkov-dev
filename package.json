{
  "name": "tarkov.dev",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prestart": "npm run prebuild",
    "start": "node scripts/start.cjs",
    "build": "node scripts/build.cjs",
    "test": "node scripts/test.cjs --env=jsdom --detectOpenHandles",
    "prettier": "prettier --write \"src/**/*.{js,jsx,mjs,ts,tsx,json,css,scss,md}\"",
    "prebuild": "node scripts/update-props.mjs && node scripts/cache-api-data.mjs && node scripts/get-version.mjs && node scripts/get-contributors.mjs && node scripts/build-sitemap.mjs && node scripts/generate-thumbnails.mjs",
    "postbuild": "node scripts/build-redirects.mjs && node scripts/critical.mjs",
    "stage": "npx rimraf build && npm run build && npm run preview",
    "preview": "npx serve build -l 3001 -s",
    "critical": "node scripts/critical.mjs",
    "cache-api-data": "node scripts/cache-api-data.mjs"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/plugin-transform-private-property-in-object": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/react-dev-utils": "^9.0.15",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "react-dev-utils": "^12.0.1",
    "source-map-explorer": "^2.5.3",
    "webpack": "^5.91.0",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@marsidev/react-turnstile": "^0.5.4",
    "@mdi/js": "^6.9.96",
    "@mdi/react": "^1.6.1",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@mui/x-tree-view": "^6.17.0",
    "@reduxjs/toolkit": "^2.2.3",
    "@svgr/webpack": "^8.1.0",
    "@tippyjs/react": "^4.2.6",
    "babel-plugin-named-asset-import": "^0.3.8",
    "bfj": "^8.0.0",
    "browserslist": "^4.23.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "classnames": "^2.5.1",
    "critical": "^6.0.5",
    "cross-fetch": "^4.0.0",
    "css-loader": "^7.0.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "fast-deep-equal": "^3.1.3",
    "file-loader": "^6.2.0",
    "framer-motion": "^11.0.24",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.0.0",
    "html-webpack-plugin": "^5.6.0",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.0",
    "identity-obj-proxy": "^3.0.0",
    "intersection-observer": "^0.12.2",
    "jest-resolve": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "leaflet": "^1.9.4",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet.awesome-markers": "2.0.5",
    "lodash.debounce": "^4.0.8",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.5.4",
    "prompts": "^2.4.2",
    "rc-slider": "^10.5.0",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-cookie-consent": "^9.0.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^4.5.0",
    "react-i18next": "^14.1.0",
    "react-intersection-observer": "^9.8.1",
    "react-loader-spinner": "^6.1.6",
    "react-query": "^3.39.3",
    "react-redux": "^9.1.0",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.22.3",
    "react-router-hash-link": "^2.4.3",
    "react-select": "^5.8.0",
    "react-simple-image-viewer": "1.2.2",
    "react-switch": "^7.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-table": "^7.8.0",
    "react-zoom-pan-pinch": "^3.4.4",
    "resize-observer-polyfill": "^1.5.1",
    "resolve": "^1.22.8",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^14.1.1",
    "semver": "^7.6.0",
    "sharp": "^0.33.3",
    "source-map-loader": "^5.0.0",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "victory": "^37.0.2",
    "webpack": "^5.91.0",
    "webpack-dev-server": "^5.0.4",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "overrides": {
    "@pmmmwh/react-refresh-webpack-plugin": {
      "webpack-dev-server": "^5.0.2",
      "rollup-plugin-terser": "@rollup/plugin-terser@^0.4.1"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.cjs",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.cjs",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.cjs"
    },
    "transformIgnorePatterns": [],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "react-hooks"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "babelOptions": {
        "presets": [
          "@babel/preset-react"
        ]
      },
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off",
      "react/jsx-key": "off",
      "react-hooks/rules-of-hooks": "off",
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4
  }
}
