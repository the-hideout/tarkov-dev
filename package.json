{
  "name": "tarkov.dev",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#src/*": "./src/*",
    "#scripts/*":"./scripts/*"
  },
  "exports": "./index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prestart": "npm run prebuild",
    "start": "rsbuild start",
    "build": "rsbuild build",
    "test": "rstest",
    "prettier": "prettier --write \"src/**/*.{js,jsx,mjs,ts,tsx,json,css,scss,md}\"",
    "eject": "react-scripts eject",
    "prebuild": "node scripts/update-props.mjs && node scripts/cache-api-data.mjs && node scripts/get-version.mjs && node scripts/get-contributors.mjs && node scripts/build-sitemap.mjs && node scripts/generate-thumbnails.mjs",
    "postbuild": "node scripts/build-redirects.mjs && node scripts/critical.mjs",
    "stage": "npx rimraf build && npm run build && npm run preview",
    "preview": "npx serve build -l 3001 -s",
    "critical": "node scripts/critical.mjs",
    "cache-api-data": "node scripts/cache-api-data.mjs"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "prettier": "^3.7.3",
    "sharp": "^0.34.5",
    "@rsbuild/core": "^1.6.11",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rsbuild/plugin-svgr": "^1.2.2",
    "@rstest/core": "^0.7.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.1",
    "@marsidev/react-turnstile": "^1.3.1",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@mui/lab": "^7.0.0-beta.17",
    "@mui/material": "^7.3.5",
    "@mui/x-tree-view": "^8.19.0",
    "@reduxjs/toolkit": "^2.11.0",
    "@tanstack/react-query": "^5.90.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "classnames": "^2.5.1",
    "critical": "^7.2.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^12.23.24",
    "fuse.js": "^7.1.0",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "intersection-observer": "^0.12.2",
    "leaflet": "^1.9.4",
    "leaflet-fullscreen": "^1.0.2",
    "lodash.debounce": "^4.0.8",
    "lz-string": "^1.5.0",
    "react": "^18.3.1",
    "react-cookie-consent": "^9.0.0",
    "react-countdown": "^2.3.6",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-i18next": "^15.4.1",
    "react-intersection-observer": "^10.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.2",
    "react-router-hash-link": "^2.4.3",
    "react-select": "^5.10.2",
    "react-simple-image-viewer": "1.2.2",
    "react-spinners": "^0.17.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-table": "^7.8.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "resize-observer-polyfill": "^1.5.1",
    "source-map-explorer": "^2.5.3",
    "victory": "^37.3.6"
  },
  "jest": {
    "transformIgnorePatterns": []
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "*.html",
      "public/**/*.html",
      "build/**/*.html"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4
  }
}